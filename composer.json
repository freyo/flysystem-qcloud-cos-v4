{
    "name": "freyo/flysystem-qcloud-cos-v4",
    "description": "Flysystem Adapter for Tencent Qcloud COS SDK V4",
    "keywords": ["flysystem-adapter", "qcloud-sdk", "qcloud", "qcloud-cos"],
    "require": {
        "php": ">=5.5.0",
        "league/flysystem": "~1.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "php": ">=5.5.0",
        "phpunit/phpunit": "~4.8"
    },	
    "autoload": {
        "psr-4": {
            "Freyo\\Flysystem\\QcloudCOSv4\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freyo\\Flysystem\\QcloudCOSv4\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "freyo",
            "email": "freyhsiao@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Freyo\\Flysystem\\QcloudCOSv4\\ServiceProvider"
            ]
        }
    }
}
